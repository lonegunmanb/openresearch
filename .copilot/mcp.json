{
  "mcpServers": {
    "playwright": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"@playwright/mcp@0.0.25",
				"--user-data-dir=./browser_profile"
			]
		},
    "filesystem": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@anthropic-ai/mcp-server-filesystem",
        "/workspaces/openresearch"
      ]
    }
  }
}
